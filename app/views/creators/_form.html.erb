<%= simple_form_for(@creator) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :dataset_id %>
    <%= f.input :family_name %>
    <%= f.input :given_name %>
    <%= f.input :institution_name %>
    <%= f.input :identifier %>
    <%= f.input :type %>
    <%= f.input :row_order %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
