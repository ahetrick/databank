<p id="notice"><%= notice %></p>

<h1>Listing Creators</h1>

<table>
  <thead>
    <tr>
      <th>Dataset</th>
      <th>Family name</th>
      <th>Given name</th>
      <th>Institution name</th>
      <th>Identifier</th>
      <th>Type</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creators.each do |creator| %>
      <tr>
        <td><%= creator.dataset_id %></td>
        <td><%= creator.family_name %></td>
        <td><%= creator.given_name %></td>
        <td><%= creator.institution_name %></td>
        <td><%= creator.identifier %></td>
        <td><%= creator.type %></td>
        <td><%= creator.row_order %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


