.modal.fade(id="licenseModal")
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        The material presented here is for informational purposes only and not for the purpose of providing legal advice.
        %button(class="close" data-dismiss="modal")
          &times;
      .modal-body
        =render partial: 'license_infographic'
        .container-fluid
          .pull-right
            %br
            Unsure?
            %button(onclick='window.location = "/help#license"' type="button" class="btn btn-success" data-dismiss="modal" id="help-button")
              %span(class="glyphicon glyphicon-question-sign")
              Get Help





