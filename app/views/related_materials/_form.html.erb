<%= simple_form_for(@related_material) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :materialType %>
    <%= f.input :availability %>
    <%= f.input :link %>
    <%= f.input :uri %>
    <%= f.input :uri_type %>
    <%= f.input :citation %>
    <%= f.input :dataset_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
