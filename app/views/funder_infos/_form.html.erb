<%= simple_form_for(@funder_info) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code %>
    <%= f.input :name %>
    <%= f.input :identifier %>
    <%= f.input :display_position %>
    <%= f.input :identifier_scheme %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
